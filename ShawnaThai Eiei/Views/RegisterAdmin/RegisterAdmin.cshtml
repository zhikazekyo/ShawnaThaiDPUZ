@model List<ShawnaThai_Eiei.Models.Cooperative>

<!DOCTYPE html>
<html lang="en">

<head>

</head>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<body id="page-top">

    <header class="masthead">
        <div class="container">
            <br><br><br><br><br><br><br><br>


            <div class="intro-heading"><font size=7>ลงทะเบียนผู้ดูแลสหกรณ์</font></div>

            <center>

            <form id="contactForm" name="sentMessage" novalidate>

                <div class="form-group">

                    <div class="col-md-6">

                        <select class="form-control custom-select" input id="Coop_Name" name="Coop_Name" style="width:200px;">
                            @foreach (var item in Model)
                            {
                                <option value="@item.Coop_Name">@item.Coop_Name</option>
                            }
                        </select>

                        <div class="form-group">
                            <input class="form-control" id="AD_ID" name="AD_ID" type="text" placeholder="ชื่อผู้ใช้ของผู้ดูแล*" required="required">
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                            <input class="form-control" id="AD_Password" name="AD_Password" type="password" placeholder="พาสเวิร์ด*" required="required">
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                            <input class="form-control" id="AD_Name" name="AD_Name" type="text" placeholder="ชื่อ* " required="required">
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                            <input class="form-control" id="AD_Lastname" name="AD_Lastname" type="text" placeholder="นามสกุล* " required="required">
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                            <input class="form-control" id="AD_Tel" name="AD_Tel" type="tel" placeholder="เบอร์โทรศัพท์* " required="required">
                            <p class="help-block text-danger"></p>
                        </div>




                        <div class="clearfix"></div>
                        <div class="col-lg-12 text-center">
                            <div id="success"></div>
                            <button id="sendMessageButton" class="btn btn-xl" type="submit">ลงทะเบียน</button>
                            <br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
                        </div>
                    </div>
                </div>
            </form>
                </center>
        </div>

    </header>
</body>
}
</html>


